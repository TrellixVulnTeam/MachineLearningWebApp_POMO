# Customises the list of options given by the GenericObjectEditor
# for various superclasses.
#
# Version: $Revision$

# Whether to use the dynamic approach or not (true|false). In case of
# "false" the old GenericObjectEditor.props file is used:
UseDynamic=true

# Lists the ResultProducers-Packages I want to choose from
com.weka.experiment.ResultProducer=\
  com.weka.experiment

# Lists the ResultListeners-Packages I want to choose from
com.weka.experiment.ResultListener=\
  com.weka.experiment

# Lists the SplitEvaluators-Packages I want to choose from
com.weka.experiment.SplitEvaluator=\
  com.weka.experiment

# Lists the Classifiers-Packages I want to choose from
com.weka.classifiers.Classifier=\
 com.weka.classifiers.bayes,\
 com.weka.classifiers.functions,\
 com.weka.classifiers.lazy,\
 com.weka.classifiers.meta,\
 com.weka.classifiers.meta.nestedDichotomies,\
 com.weka.classifiers.mi,\
 com.weka.classifiers.misc,\
 com.weka.classifiers.scripting,\
 com.weka.classifiers.trees,\
 com.weka.classifiers.rules

# Lists the IterativeClassifier objects I want to choose from
com.weka.classifiers.IterativeClassifier=\
 com.weka.classifiers.functions,\
 com.weka.classifiers.meta,\
 com.weka.classifiers.trees
 
# Lists the AbstractClassificationOutput-packages I want to choose from
com.weka.classifiers.evaluation.com.output.prediction.AbstractOutput=\
  com.weka.classifiers.evaluation.com.output.prediction

# Lists the Filters-Packages I want to choose from
com.weka.filters.Filter= \
 com.weka.filters, \
 com.weka.filters.supervised.attribute, \
 com.weka.filters.supervised.instance, \
 com.weka.filters.unsupervised.attribute, \
 com.weka.filters.unsupervised.instance

# Lists the UnsupervisedFilters-Packages I want to choose from
com.weka.filters.UnsupervisedFilter = \
 com.weka.filters.unsupervised.attribute, \
 com.weka.filters.unsupervised.instance

# Lists the Attribute Selection Evaluators-Packages I want to choose from
com.weka.attributeSelection.ASEvaluation = \
 com.weka.attributeSelection

# Lists the Attribute Selection Search methods-Packages I want to choose from
com.weka.attributeSelection.ASSearch = \
 com.weka.attributeSelection

# Lists the Associators-Packages I want to choose from
com.weka.associations.Associator=\
 com.weka.associations

# Lists the Clusterers-Packages I want to choose from
