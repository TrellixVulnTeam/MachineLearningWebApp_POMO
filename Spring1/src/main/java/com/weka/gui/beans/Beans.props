# list of standard toolbars (containing bean tools that do not wrap com.weka
# base class types)
com.weka.gui.beans.KnowledgeFlow.standardToolBars=\
 Evaluation,\
 Visualization,\
 Tools,\
 Flow

# Specifies the tools for each standard toolbar
com.weka.gui.beans.KnowledgeFlow.Evaluation=com.weka.gui.beans.TrainingSetMaker,\
 com.weka.gui.beans.TestSetMaker,\
 com.weka.gui.beans.CrossValidationFoldMaker,\
 com.weka.gui.beans.TrainTestSplitMaker,\
 com.weka.gui.beans.ClassAssigner,\
 com.weka.gui.beans.ClassValuePicker,\
 com.weka.gui.beans.ClassifierPerformanceEvaluator,\
 com.weka.gui.beans.IncrementalClassifierEvaluator,\
 com.weka.gui.beans.ClustererPerformanceEvaluator,\
 com.weka.gui.beans.PredictionAppender,\
 com.weka.gui.beans.SerializedModelSaver,\
 com.weka.gui.beans.TextSaver
 
com.weka.gui.beans.KnowledgeFlow.Visualization=com.weka.gui.beans.DataVisualizer,\
 com.weka.gui.beans.ScatterPlotMatrix,\
 com.weka.gui.beans.AttributeSummarizer,\
 com.weka.gui.beans.ModelPerformanceChart,\
  com.weka.gui.beans.CostBenefitAnalysis,\
 com.weka.gui.beans.TextViewer,\
 com.weka.gui.beans.GraphViewer,\
 com.weka.gui.beans.StripChart,\
 com.weka.gui.beans.ImageSaver,\
 com.weka.gui.beans.ImageViewer
 com.weka.gui.beans.KnowledgeFlow.Tools=com.weka.gui.beans.Sorter,\
 com.weka.gui.beans.SubstringLabeler,\
 com.weka.gui.beans.SubstringReplacer
com.weka.gui.beans.KnowledgeFlow.Flow=com.weka.gui.beans.Appender,\
 com.weka.gui.beans.FlowByExpression,\
 com.weka.gui.beans.InstanceStreamToBatchMaker,\
 com.weka.gui.beans.Join

# the bean panel processes keys in the generic object editor properties file.
# If there is a corresponding entry here, then a toolbar is constructed using
# the specified wrapper bean
com.weka.core.converters.Loader=com.weka.gui.beans.Loader
com.weka.core.converters.Saver=com.weka.gui.beans.Saver
com.weka.classifiers.Classifier=com.weka.gui.beans.Classifier
com.weka.filters.Filter=com.weka.gui.beans.Filter
com.weka.clusterers.Clusterer=com.weka.gui.beans.Clusterer
com.weka.associations.Associator=com.weka.gui.beans.Associator
# com.weka.attributeSelection.ASEvaluation=com.weka.gui.beans.AttributeSelector

# toolbar ordering information for wrapper types
com.weka.core.converters.Loader.order=0
com.weka.core.converters.Saver.order=1
com.weka.filters.Filter.order=3
com.weka.classifiers.Classifier.order=4
com.weka.clusterers.Clusterer.order=5
com.weka.associations.Associator.order=6
# com.weka.attributeSelection.ASEvaluation.order=6

# toolbar naming aliases for com.weka algorithm classes
com.weka.core.converters.Loader.alias=DataSources
com.weka.core.converters.Saver.alias=DataSinks
com.weka.classifiers.Classifier.alias=Classifiers
com.weka.clusterers.Clusterer.alias=Clusterers
com.weka.filters.Filter.alias=Filters
com.weka.associations.Associator.alias=Associations

# GUI behaviour
ScrollBarIncrementLayout=20
ScrollBarIncrementComponents=50
FlowWidth=2560
FlowHeight=1440
PreferredExtension=.kfml
UserComponentsInXML=false

# colours
com.weka.gui.beans.StripChart.backgroundColour=black
com.weka.gui.beans.StripChart$LegendPanel.borderColour=blue
